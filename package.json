{
  "name": "sholat-reminder",
  "displayName": "Sholat Reminder",
  "icon": "media/icon.png",
  "license": "SEE LICENSE IN LICENSE.md",
  "description": "",
  "version": "1.0.14",
  "author": "Aditya Putra Pratama",
  "publisher": "AdityaPutraPratama",
  "engines": {
    "vscode": "^1.95.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adityaputra11/sholat-reminder.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.setCityID",
        "title": "Sholat Reminder: Set City ID"
      },
      {
        "command": "extension.searchCity",
        "title": "Sholat Reminder: Search City"
      },
      {
        "command": "extension.toogleCityName",
        "title": "Sholat Reminder: Toggle City Name"
      },
      {
        "command": "extension.refreshSholatSchedule",
        "title": "Sholat Reminder: Refresh Sholat Schedule"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/mocha": "^10.0.9",
    "@types/node": "^20.17.6",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3",
    "vscode-test": "^1.6.1"
  },
  "dependencies": {
    "axios": "^1.7.7"
  }
}
